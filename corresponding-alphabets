/*
Input : 14523 
output: befcd [give coresponding alphabet mapped to it]
*/
#include<iostream>
#include<vector>
#include<bits/stdc++.h>
using namespace std;
int main(void)
{
	int a,d;
	vector<int>v,v1;

	vector<char>v2;
	cin>>a;
	v.push_back(a);
	for(auto x:v)
	{
int d=x;
    for(; d; d/=10)
    v1.push_back( d%10 );	
	
		reverse(v1.begin(),v1.end());


	}
	for(auto x:v1)
	{
	char b=x+97;
	v2.push_back(b);
	}	
	for(auto x:v2)
	cout<<x;
}
